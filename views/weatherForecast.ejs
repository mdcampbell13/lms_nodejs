  <body class="container bg-dark mt-1">
    <h1 class="display-4 text-light text-center">Forecast for <%= weatherZipInfo.city %>, <%= weatherZipInfo.state %> <%= weatherZipInfo.zip_code %></h1>
    <div class="container mt-5">
              <div class="card mb-3 ml-auto mr-auto" style="max-width: 540px;">
            <div class="row no-gutters">
              <div class="col-md-4">
                <img src="http://openweathermap.org/img/wn/<%= sevenDayForecast.current.weather[0].icon %>@2x.png" width="125" height="125" alt="weather icon" id="currentIcon">
                <h2 class="text-center"><strong><%= parseInt(sevenDayForecast.current.temp) %>&deg</strong></h2>
              </div>
              <div class="col-md-8">
                <div class="card-body bg-secondary text-light">
                  <h5 class="card-title text-center">Currently</h5>
                  <p class="card-text text-center wc"><%= sevenDayForecast.current.weather[0].main %></p>
                  <p class="card-text text-center wc2"><%= sevenDayForecast.current.weather[0].description %></p>
                  <p class="card-text text-center wc2">Chance of Rain: <%= parseInt(sevenDayForecast.hourly[0].pop*100) %>%</p>
                  <p class="card-text text-center wc">Wind Speed: <%= parseInt(sevenDayForecast.current.wind_speed) %> MPH</p>
                  <p class="card-text text-center wc2">Wind Direction: <%= sevenDayForecast.current.wind_deg %>&deg</p>
                  <p class="card-text text-center wc">Wind Speed: <%= parseInt(sevenDayForecast.current.wind_speed) %> MPH</p>
                  <p class="card-text text-center wc2">Wind Direction: <%= sevenDayForecast.current.wind_deg %>&deg</p>
                  <p class="card-text text-center wc">Sunrise: <%= sevenDayForecast.current.sunrise_time %></p>
                  <p class="card-text text-center wc">Sunset: <%= sevenDayForecast.current.sunset_time %></p>
                </div>
              </div>
            </div>
          </div>
    </div>
